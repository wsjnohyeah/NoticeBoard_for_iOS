# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'NoticeBoard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NoticeBoard
  pod 'Alamofire', '~> 4.4'
  pod 'SwiftyJSON'
  pod 'Kingfisher', '~> 3.0'
  pod 'Agrume', :git => 'https://github.com/wsjnohyeah/Agrume.git'

end